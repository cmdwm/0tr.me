  <script>
    document.getElementById("year").innerHTML = new Date().getFullYear();

    function slDecode() {
        fetch('/decode?slug=' + document.getElementById('decSl').value.replace('0tr.me/', '').replace('https://', '').replace('http://', '') )
          .then(response => response.text())
          .then(data => Swal.fire({
            imageUrl: `https://image.thum.io/get/width/900/crop/500/${data}`,
//imageUrl: `https://processor.screenshotson.click/screenshot?url=${data}&width=1280&height=1024&fullPage=false&quality=70&loadEvent=load&fileType=jpeg`, //document.getElementById('decSl'),
  text: data
}))
          .catch(error => console.error(error));
      }
    // Get all the elements with the class "imgDir"
var elements = document.getElementsByClassName("imgDir");

// Loop through the elements and add a click event listener
for (var i = 0; i < elements.length; i++) {
  // Set the title attribute for each element
  elements[i].setAttribute("title", "Click to view larger image");

  elements[i].addEventListener("click", function() {
    // Get the source of the clicked image
    var src = this.getAttribute("src");
    
    // Open the source image in a new tab
    window.open(src);
  });
}

    if (navigator.clipboard) {
  // Clipboard API is supported
  navigator.clipboard.readText().then(text => {
    if(text.includes('http')) {
      document.getElementById('urlF').value = text
    }
  });
} else {
  // Clipboard API is not supported
 console.log('Clipboard API is not supported by this browser, so nothing was pasted. If you see this message, use an up to date browser so you don\'t need to manually paste anymore!');
}
    function fileSwitch() {
  
var element = document.querySelector("#url");
if (element.style.display === "none") {
  document.getElementById('uStatus').innerHTML = `<i class="fa-solid fa-link"></i>`
  element.style.display = "block";
} else {
  element.style.display = "none";

}
var file = document.querySelector("#file");
if (file.style.display === "none") {
  document.getElementById('uStatus').innerHTML = `<i class="fa-solid fa-file-arrow-up"></i>`
  file.style.display = "block";
} else {
  file.style.display = "none";

}
}
var payMod = document.getElementById("payMod");
var modBtn = document.getElementById("openPayMod");
var closeBtn = document.getElementsByClassName("cancelBtn")[0];

// When the user clicks the button, open the modal 
modBtn.onclick = function() {
  payMod.style.display = "block";
  document.getElementById('payLongVal').value = document.getElementById('urlF').value
}

// When the user clicks on <span> (x), close the modal
closeBtn.onclick = function() {
  payMod.style.display = "none";
}

// When the user clicks anywhere outside of the modal, close it
window.onclick = function(event) {
  if (event.target == payMod) {
    payMod.style.display = "none";
  }
}
